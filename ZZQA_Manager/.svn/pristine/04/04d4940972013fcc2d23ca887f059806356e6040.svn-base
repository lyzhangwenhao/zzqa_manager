/****我的通知样式开始***/
.div_title{
	text-align: center;
    margin: 30px auto 10px auto;
    color: #000;
    font: 25px/30px 'Microsoft yahei';
}
.add_notify_btn{
	float:right;
	margin:0px 12px 2px 0;
	width:90px;
	cursor:pointer;
	text-decoration:none;
	color:#fff;
	background:#2BABFF;
	text-align:center;
	font: 14px/28px 'Microsoft yahei';
	border-radius:3px;
}
.add_notify_btn:hover{
	background:#279ce8;
}
.div_dashed{
	margin: 50px 0 5px 0;
	border-bottom: 1px dashed #2ABDFF;
	width:100%;
}
.notify_list {
	margin:0 0 20px 0;
}
.notify_item{
	font: 15px/50px 'Microsoft yahei';
	border-bottom:1px solid #e5e5e5;
	overflow: hidden;
    zoom: 1;
}
.notify_item:hover {
    background: #D8EDFF;
}
.notify_title{
	float:left;
 	width:450px;
	 white-space: nowrap;
    -o-text-overflow: ellipsis;
    -moz-text-overflow: ellipsis;
    text-overflow: ellipsis;
    overflow: hidden;
    margin:0 30px 0 120px;
    cursor:pointer;
}
.notify_date{
	float:left;
	width:85px;
	cursor:pointer;
}
.notify_item a{
	float:right;
	margin:0px 12px;
	cursor:pointer;
	color:#2ABDFF;
	text-decoration:none;
	text-align:center;
	outline:none;
}
.notify_item a:hover{
	text-decoration:underline;
}
.div_line{
	float:right;
	width:1px;
	margin:19px 0px 0 0px;
	height:15px;
	background:#2ABDFF;
}
/****我的通知样式结束***/
