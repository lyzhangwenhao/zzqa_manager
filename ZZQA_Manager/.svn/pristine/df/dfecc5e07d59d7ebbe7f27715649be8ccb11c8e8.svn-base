/**流程列表样式开始**/
.td2_div1{
	text-align:center;
	margin:20px auto;
	color:#000;
	font:20px/30px 'SimSun';
	font-weight:700;
}
.td2_table0{
	border:0px;
	border-collapse: collapse;
	margin:30px auto;
	width:85%;
	border-top:1px solid #878787;
	border-left:1px solid #878787;
}
.td2_table0 td{
	font:14px/30px 'SimSun';
	padding:0px 5px 0px 5px; 
	border-bottom:1px solid #878787;
	border-right:1px solid #878787;
}
.table0_tr1{
	height:45px;
}
.table0_tr1_td1{
	text-align:center;
	width:1%;
	min-width:100px;
}
.table0_tr1_td2{
	text-align:left;
	width:99%;
}
.td2_table1{
	border:0px;
	border-collapse: collapse;
	margin:20px auto;
	width:100%;
	border-top:1px solid #878787;
	border-left:1px solid #878787;
}
.td2_table1 td{
	font:14px/30px 'SimSun';
	padding:0px 5px 0px 5px; 
	border-bottom:1px solid #878787;
	border-right:1px solid #878787;
}
.table1_tr1{
	
}
.table1_tr1_td1{
	text-align:center;
}
.table1_tr1_td2{
	text-align:center;
}
.table1_tr1_td3{
	text-align:center;
}
.table1_tr1_td4{
	text-align:center;
	width:50px;
}
.div_search{
	margin: 10px auto; 
	height: 400px; 
	width: 85%;
	box-sizing: border-box;<!--边框、内边距包含在宽高内-->
	-moz-box-sizing: border-box;
	-webkit-box-sizing: border-box;
}
.div_search div{
	box-sizing: border-box;<!--边框、内边距包含在宽高内-->
	-moz-box-sizing: border-box;
	-webkit-box-sizing: border-box;
}
.search_left{
	float: left; 
	width: 43%; 
	height: 100%;
	border: 1px solid #878787;
}
.search_center{
	float: left; 
	width:14%; 
	height: 100%; 
	position: relative;
}
.search_center input{
	font: 20px/ 25px 'microsoft yahei'; 
	height: 30px; 
	width: 40px; 
	display: block;
	padding: 0px;
}
.search_input_top{
	margin:145px auto 0px auto;
}
.search_input_bottom{
	margin:50px auto 0px auto;
}
.search_right{
	float: left; 
	width:43%; 
	height: 100%; 
	border: 1px solid #878787;
}
.search_lefttop{
	height: 45px; 
	border-bottom: 1px solid #878787; 
	text-align: left;
}
.search_lefttop span{
	margin-left: 10px; 
	font: 14px/ 45px 'microsoft yahei';
}
.search_lefttop input{
	font: 13px/ 25px 'microsoft yahei'; 
	margin-left: 10px;
	padding: 0 0 0 5px; 
	height: 30px; 
	width: 170px;
}
.search_leftbottom{
	height: 353px; 
	text-align: left; 
	overflow-y: auto;
}
.search_leftbottom div{
	text-align: center;
	font: 14px/ 30px 'microsoft yahei'
}
.search_leftbottom li label{
	display:block;
	width:100%;
	font: 14px/ 30px 'microsoft yahei';
	cursor:pointer;
}
.search_leftbottom li:hover{
	background-color:#dfebfe;
}
.search_leftbottom input{
	position: relative; 
	top: 2px; 
	padding: 0px; 
	margin: 0px;
}
.search_righttop{
	height: 45px; 
	border-bottom: 1px solid #878787; 
	text-align: left;
}
.search_righttop span{
	margin-left: 10px; 
	font: 14px/ 45px 'microsoft yahei';
}
.search_rightbottom{
	height: 353px; 
	text-align: left; 
	overflow-y: auto;
}
.search_rightbottom div{
	width:100%;
	text-align: center;
}
.search_rightbottom li,label{
	display:block;
	width:100%;
	font: 14px/ 30px 'microsoft yahei';
	cursor:pointer;
}
.search_rightbottom li:hover{
	background-color:#dfebfe;
}
.search_rightbottom input{
	position: relative;
	top: 2px; 
	padding: 0px;
	margin: 0px;
}

/**新建任务单流程样式结束**/

