/**�Ի�����ʽ��ʼ**/
.dialog_work_bg{
	width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    opacity: 0.3;
    filter: alpha(opacity=0.3);
    background-color: #000000;
    z-index: 1;
    border: 1px solid #e5e5e5;
}
.dialog_pm{
	border: 1px solid #2CABF4;
    width: 400px;
    background: #fff;
    position: fixed;
    top:50%;
    left: 50%;
    margin-left: -200px;
    z-index: 2;
    display: none;
}
.dialog_pm_title{
	background:#2CABF4;
	font: 16px/23px 'microsoft yahei';
	zoom:1;
	overflow: hidden;
}
.dialog_pm_title>div:nth-of-type(1){
	float: left;
	color:#fff;
	margin-left:10px;
}
.dialog_pm_title>div:nth-of-type(2){
    float: right;
    height: 16px;
    width: 16px;
    margin-top: 4px;
    border: 0px;
    margin-right:10px;
    background-image: url(../images/whilte_close.png);
    cursor:pointer;
}
.dialog_pm_title>div:nth-of-type(2):hover{
    background-image: url(../images/red_close.png);
}
.dialog_pm_list{
	margin-bottom:10px;
	max-height: 280px;
	overflow-y:auto; 
}
.dialog_pm_item{
	margin-top:20px;
}
.dialog_pm_item:nth-child(odd){
	background:#E6E6E6;
}
.dialog_pm_item:nth-child(even){
	background:#D2F1FD;
}
.dialog_pm_item:hover{
	background:#DEF5B5;
}
.dialog_pm_item>input{
	float: left;
    margin-left: 51px;
    width: 191px;
    text-align: left;
    padding: 0 7px;
    font: 15px/21px 'microsoft yahei';
}
.dialog_pm_item_div1{
	float: left;
    margin-left: 60px;
    width: 200px;
    white-space: nowrap;
    -o-text-overflow: ellipsis;
    -moz-text-overflow: ellipsis;
    text-overflow: ellipsis;
    overflow: hidden;
    font: 15px/25px 'microsoft yahei';
}
.dialog_pm_item>img:nth-of-type(1){
	float:left;
	height: 23px;
    width: 23px;
    margin:1px 0 1px 15px;
    border: 0px;
    cursor:pointer;
}
.dialog_pm_item>img:nth-of-type(2){
	float:left;
	height: 23px;
    width: 23px;
    border: 0px;
    margin:1px 0 1px 25px;
    cursor:pointer;
}
.clearfloat_div{
	clear:both;
}
.dialog_pm_additem{
	margin:10px 0;
}
.dialog_pm_additem>input{
	float: left;
    margin-left: 50px;
    width: 196px;
    text-align:left;
    padding:0 5px;
    font: 15px/21px 'microsoft yahei';
}
.dialog_pm_additem>img{
	float:left;
	height: 23px;
    width: 23px;
    margin:1px 0 1px 64px;
    border: 0px;
    cursor:pointer;
}
.dialog_pm_btns{
	border-top:1px solid #2CABF4;
	text-align:center;
}
.dialog_pm_btns>div{
	display:inline-block;
	margin:10px 10px; 
	width:100px;
	color:#fff;
	font: 16px/25px 'microsoft yahei';
	border-radius:3px;
	cursor:pointer;
}
.dialog_pm_btns>div:nth-of-type(1){
	background:#30A5FF;
}
.dialog_pm_btns>div:nth-of-type(2){
	background:#4CB6AC;
}
.dialog_pm_btns>div:nth-of-type(3){
	background:#F49E52;
}
.dialog_workday{
	border: 1px solid #2CABF4;
    width: 500px;
    background: #fff;
    position: fixed;
    top:50%;
    left:50%;
    margin-left: -250px;
    z-index: 2;
    padding: 0 0 20px 0;
    display: none;
}
.dialog_workday_title{
	zoom:1;
	overflow: hidden;
}
.dialog_workday_title>div{
    float: right;
    height: 16px;
    width: 16px;
    border: 0px;
    margin:10px 10px 10px 0;
    background-image: url(../images/black_close.png);
    cursor:pointer;
}
.dialog_workday_title>div:hover{
    background-image: url(../images/red_close.png);
}
.dialog_workday_list{
	max-height: 200px;
	overflow-y:auto; 
	margin-bottom:10px;
}
.dialog_workday_date{
	width: 400px;
    margin: 10px auto 0px auto;
    text-align: left;
    margin-left: 55px;
    font: 15px/25px 'microsoft yahei';
    color: #FF9999
}
.dialog_workday_item{
	width: 400px;
    margin: 10px auto 0px auto;
    text-align: left;
    margin-left: 55px;
    font: 15px/25px 'microsoft yahei';
}
.item_title1{
	float:left;
	width:50px;
}
.dialog_workday_item select{
	float: left;
    width: 150px;
    height: 25px;
}
.item_pname{
	float:left;
	width:152px;
	white-space: nowrap;
    -o-text-overflow: ellipsis;
    -moz-text-overflow: ellipsis;
    text-overflow: ellipsis;
    overflow: hidden;
}
.item_title2{
	float:left;
	width: 50px;
    margin-left: 15px;
}
.dialog_workday_item input{
	float: left;
    width: 41px;
    text-align: left;
    padding: 0 5px;
    height: 21px;
    margin-right: 5px;
}
.item_hours{
	float:left;
	width:60px;
	text-align: center;
	position: relative;
    left: -5px;
}
.item_title3{
	float:left;
	width:20px;
}
.workday_item_del{
	float:left;
	height: 16px;
    width: 16px;
    margin-top: 4px;
    border: 0px;
    margin-left:15px;
    background-image: url(../images/delete_project.png);
    cursor:pointer;
}
.workday_item_add{
	float:left;
	height: 16px;
    width: 16px;
    margin-top: 4px;
    margin-left:5px;
    border: 0px;
    background-image: url(../images/add_project.png);
    cursor:pointer;
}
.dialog_workday_content{
	font: 15px/18px 'microsoft yahei';
    margin: 15px 0 15px 55px;
    text-align: left;
    overflow: hidden;
}
.dialog_workday_content>div:nth-of-type(1){
	float:left;
	width:50px;
}
.dialog_workday_content>textarea{
	float: left;
    display: block;
    width: 336px;
    height:90px;
    padding: 2px 5px;
    font: 14px/20px 'microsoft yahei';
    border: 1px solid #878787;
    outline: none;
}
.dialog_workday_content>div:nth-of-type(2){
	float: left;
    display: block;
    max-height: 90px;
    width: 348px;
    padding: 0;
    font: 14px/20px 'microsoft yahei';
    overflow-y: auto;
}
.dialog_workday_remark{
	font: 15px/18px 'microsoft yahei';
    margin: 15px 0 15px 55px;
    text-align: left;
    overflow: hidden;
}
.dialog_workday_remark>div:nth-of-type(1){
	float:left;
	width:50px;
}
.dialog_workday_remark>textarea{
	float: left;
    display: block;
    height: 90px;
    width: 336px;
    padding: 2px 5px;
    font: 14px/20px 'microsoft yahei';
    border-left: 1px solid #878787;
    outline: none;
}
.dialog_workday_remark>div:nth-of-type(2){
	float: left;
    display: block;
    max-height: 90px;
    width: 348px;
    padding: 0;
    font: 14px/20px 'microsoft yahei';
    overflow-y: auto;
}
.dialog_workday_btns{
	margin: 10px auto 0 auto;
    text-align: center;
}
.dialog_workday_btns>div {
    display: inline-block;
    width: 100px;
    color: #fff;
    font: 16px/25px 'microsoft yahei';
    border-radius: 3px;
    cursor: pointer;
    background: #30A5FF;
    color:#fff;
}
