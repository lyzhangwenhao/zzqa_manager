}
.track_bottom_left{
	float:left;
	width:60%;
}
.track_bottom_right{
	float:right;
	width:40%;
	text-align:right;
}
.track_bottom_left1{
	float:left;
	text-align:left;
	margin-right:35px;
	font:15px/25px 'Microsoft yahei';
}
.track_bottom_left1 div{
	text-align:left;
}
.track_bottom_left1>div span:nth-of-type(2){
	margin-left:10px;
	display:inline-block;
}
.track_bottom_right1{
	text-align:right;
	font:15px/20px 'Microsoft yahei';
	height:22px;
	zoom:1;
	overflow:hidden;
}
.chk_2 { 
    display: none; 
}
.chk_2+ label {
	float:right;
    background-color: #FFF; 
    border: 1px solid #C1CACA; 
    box-shadow: 0 1px 2px rgba(0, 0, 0, 0.05), inset 0px -15px 10px -12px rgba(0, 0, 0, 0.05); 
    width:18px;
    height:18px;
    border-radius: 3px; 
    display: inline-block;
    margin-right: 5px;
    text-align: center;
    cursor:pointer;
}
.chk_2 + label:active { 
    box-shadow: 0 1px 2px rgba(0,0,0,0.05), inset 0px 1px 3px rgba(0,0,0,0.1); 
}
.chk_2:checked + label { 
    background-color: #ECF2F7; 
    border: 1px solid #92A1AC; 
    box-shadow: 0 1px 2px rgba(0, 0, 0, 0.05), inset 0px -15px 10px -12px rgba(0, 0, 0, 0.05), inset 15px 10px -12px rgba(255, 255, 255, 0.1); 
    color: #243441;
}
.chk_2:checked + label:after { 
    content: '\2714'; /***勾选符号***/ 
    color: #758794; 
    width: 100%; 
    text-align: center; 
    font-size: 14px;
    padding: 1px 0 0 0; 
    vertical-align: text-top; 
}
.track_bottom_right1 select{
	float:right;
	height:22px;
	padding:0 10px;
	text-align:center;
	margin-right:10px;
}
.track_bottom_right1 div:nth-of-type(2){
	float:right;
	display:inline-block;
	cursor:pointer;
	margin-right: 10px;
	font:16px/22px 'microsoft yahei';
	color:#00B5FE;
}
.track_bottom_right1 div:nth-of-type(1){
	float:right;
	display:inline-block;
	border-radius:2px;
	width:70px;
	background:#23CE8C;
	color:#fff;
	text-align:center;
	cursor:pointer;
	font: 16px/22px 'microsoft yahei';
}
.track_btn{
	float:right;
	margin:10px 0 0 0;
	border-radius:2px;
	font: 15px/30px 'microsoft yahei';
	width:145px;
	background:#2BA2FF;
	text-align:center;
	color:#fff;
	cursor:pointer;
}
.track_btn:hover{
	background:#279ce8;
}
