.chk_1 { 
    display: none; 
} 
.chk_1 + label { 
    background-color: #FFF;
    border: 1px solid #C1CACA;
    width: 18px;
    height: 18px;
    border-radius: 3px;
    display: block;
    position: relative;
    top: 3px;
    left: 62px;
    cursor:pointer;
    visibility:hidden;
}
.chk_1:checked + label {
	padding: 0px;
	display: block; 
    background-color: #ECF2F7; 
    border: 1px solid #92A1AC; 
    color: #243441;
} 
 
.chk_1:checked + label:after { 
    content: '\2714';/*****勾选符号***/
    top: 0px;
    left: 0px;
    color: #758794;
    width: 100%;
    text-align: center;
    font-size: 14px;
    vertical-align: text-top;
}

.track_date>div>div:nth-of-type(1){
	font:24px/42px 'SimSun';
	font-weight: bold;
}
.track_date>div>div:nth-of-type(2){
	font:20px/25px 'SimSun';
}
.track_date select{
	height:25px;
	width:70px;
	padding:0 5px;
	font: 18px/23px 'SimSun';
}
.track_bottom_bg{
	margin:10px auto;
	border-top:1px solid #00B5FE;
}
.track_bottom{
	margin:0 auto;
	width:85%;
	overflow: hidden;
	zoom:1;
