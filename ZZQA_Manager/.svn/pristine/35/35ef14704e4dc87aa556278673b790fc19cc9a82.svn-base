/**流程列表样式开始**/
.td2_div1{
	text-align:center;
	margin:20px auto;
	color:#000;
	font:bold 20px/30px 'SimSun';
}
/**表格**/
.td2_table2{
	margin-top:20px;
	width:100%;
	color:#000;
	font:14px/30px 'SimSun';
	border-collapse: collapse;
	border-left:1px solid #878787;
	border-top:1px solid #878787;
}
.td2_table2 label{
	font:13px 'SimSun';
}

.td2_table2 td{
	border-right:1px solid #878787;
	border-bottom:1px solid #878787;
}
.table2_tr1{
	height:40px;
}
.table2_tr1_td1{
	text-align:center;
	width:1%;
	min-width:80px;
	color:#000;
}
.table2_tr1_td2{
	width:99%;
	text-align:left;
	padding:10px;
}
.table2_tr1_td2 input{
	height:20px;
	width:30%;
	min-width:245px;
	padding:2px 5px;
}
.table2_tr1_td2 span{
	margin-left:40px;
 	height:20px;
	color:#f00;
	font:13px 'SimSun';
}
.table2_tr2{
	
}
.table2_tr2_td1{
	text-align:center;
	width:1%;
	min-width:80px;
	color:#000;
}
.table2_tr2_td2{
	width:99%;
	text-align:left;
	padding:10px;
	font:13px/20px 'SimSun';
}
.table2_tr2_td2_span{
	display:inline-block;
	height:25px;
	min-width:100px;
	font:13px/25px 'SimSun';
}
.div_padding{
	text-align:left;
	padding:5px 0px;
}
.error_span{
 	margin-left:42px;
 	height:20px;
 	color:#f00;
}
.div_padding input{
	height:20px;
	width:100px;
	margin-right:10px;
	padding:0px 5px;
}
.div_padding img{
	border: none; 
	height:15px;
	margin-left:10px;
	vertical-align:middle;
	margin-top:-3px;
	cursor:pointer;
}
.table2_tr3{

}
.table2_tr3_td1{
	text-align:center;
	width:1%;
	min-width:80px;
	color:#000;
}
.table2_tr3_td2{
	width:99%;
	text-align:left;
	padding:10px;
	font:13px/20px 'SimSun';
}
.table2_tr3_td2 input{
	height:20px;
	width:140px;
	padding:0px 5px;
}

