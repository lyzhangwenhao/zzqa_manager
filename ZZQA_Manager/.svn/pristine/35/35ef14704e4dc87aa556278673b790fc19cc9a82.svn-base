.table2_tr4_td1{
	text-align:center;
	width:1%;
	min-width:80px;
	color:#000;
}
.table2_tr4_td2{
	width:99%;
	text-align:left;
	padding:10px;
	font:13px/20px 'SimSun';
}
.table2_tr4_td2 input{
	height:20px;
	width:140px;
	padding:0px 5px;
