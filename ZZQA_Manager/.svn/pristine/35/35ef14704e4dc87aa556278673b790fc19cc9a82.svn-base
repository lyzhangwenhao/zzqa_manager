	text-align:left;
	padding:10px;
}
.div_btn{
	margin-top:20px;
	text-align:center;
}
.fistbutton{
	border: none; 
	height:35px;
	cursor:pointer;
	margin-right:50px;
}
.btn_agree{
	border: none; 
	height:35px;
	cursor:pointer;
}
.btn_disagree{
	border: none; 
	height:35px;
