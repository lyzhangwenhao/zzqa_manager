/***模拟checkbox样式开始**/
.chk_1 { 
	float:left;
    display: none; 
} 
.chk_1 + label {
	float: left;
    background-color: #FFF;
    border: 1px solid #C1CACA;
    width: 14px;
    height: 14px;
    border-radius: 3px;
    display: block;
    position: relative;
    top: 7px;
    cursor: pointer;
}
.chk_1:checked + label {
	padding: 0px;
	display: block; 
    background-color: #ECF2F7;
    border: 1px solid #92A1AC; 
    color: #243441;
}
.chk_1:checked + label:after {
    position: relative;
    content: '\2714';
    bottom: 8px;
    left: 2px;
    color: #758794;
    width: 100%;
    text-align: center;
    font-size: 14px;
    vertical-align: text-top;
}
/*****模拟checkbox样式结束****/