.td2_div1{
	text-align:center;
	margin:20px auto;
	color:#000;
	font:20px/30px 'microsoft	yahei';
	font-weight:700;
}
.td2_table{
	border:0px;
	border-collapse: collapse;
	margin:30px auto;
	width:700px;
	border-top:1px solid #878787;
	border-left:1px solid #878787;
	font:14px/30px 'microsoft	yahei';
}
.td2_table td{
	padding:0;
	border-bottom:1px solid #878787;
	border-right:1px solid #878787;
}
.table_tr1{
}
.table_tr1_td1{
	width:5%;
	min-width:100px;
}
.table_tr1_td2{
	width:45%;
	text-align:center;
}
.table_tr1_td2 select{
	padding:2px 3px;
	border:1px solid #878787;
}
.table_tr1_td3{
	width:5%;
	min-width:100px
}
.table_tr1_td4{
	width:45%;
	text-align:center;
}
.table_tr1_td4 select{
	padding:2px 3px;
	border:1px solid #878787;
}
.table_tr2{
	
}
.table_tr2_td1{
	width:1%;
	min-width:100px
	text-align:center;
}
.table_tr2_td2{
	width:99%;
	text-align:left;
}
.table_tr2_td2 div{
	float:left;
	margin-left:15px;
}
.table_tr2_td2 img{
	float:left;
	margin-left:10px;
	height:24px;
	position: relative;
    top: 2px;
    cursor:pointer;
}
.input-hide-time{
	float: left;
    width: 0px;
    display: inline-block;
    padding: 0px;
    height: 0px;
    border: 0px;
    position: relative;
    top: 25px;
    left: 10px;
    z-index: -100;
}
.table_tr2_td2 select{
	float:left;
	margin-left:10px;
	border:1px solid #878787;
	vertical-align:middle;
	position: relative;
    top: 5px;
}
.table_tr3_td1{
	width:5%;
	min-width:100px;
	text-align:center;
}
.table_tr3_td2{
	width:95%;
	text-align:center;
}
.table_tr3_td2 textarea {
	display: block;
	width:100%;
	height:80px;
	padding:0;
	margin:0;
	border:0;
}
.table_tr4{
	
}
.table_tr4_td1{
	width:1%;
	min-width:100px
	text-align:center;
}
.table_tr4_td2{
	padding:10px;
	width:99%;
	text-align:left;
}