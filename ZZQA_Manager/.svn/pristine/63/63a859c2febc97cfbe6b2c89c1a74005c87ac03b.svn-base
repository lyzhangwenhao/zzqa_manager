/**公共头部样式开始**/
body,html{
  margin:0px;
  padding:0px;
  background:#F0F4F7;
  font-size: 62.5%;
}
form,ul{
	margin:0px;
 	padding:0px;
 	list-style-type:none;
}
td,th {
	padding: 0
}
textarea{
	resize:vertical;
	min-height:30px;
	max-height: 100px;
}
div{
	word-break:break-all;
}
img { border:none;}
input[type="reset"]::-moz-focus-inner, 
input[type="button"]::-moz-focus-inner, 
input[type="submit"]::-moz-focus-inner, 
input[type="file"] > input[type="button"]::-moz-focus-inner{
	border:none;padding:0;
	outline: none;
}
input[type="reset"], 
input[type="button"], 
input[type="radio"], 
input[type="checkbox"], 
input[type="submit"]{ 
	cursor:pointer;
}
.div_top{
}
.div_top1{
	margin:0px auto;
  	padding:0px;
  	width:1092px;
  	height:24px;
  	text-align:right;
}
.div_top1_img{
	float:left;
	height:24px;
}
.div_top1 span{
	float:right;
	margin-right:2px;
	color:#757b85;
	font:13px/24px 'Microsoft Yahei';
}
.div_top1_span{
	float: right;
	color:#757b85;
	font:13px/24px 'Microsoft Yahei';
	text-decoration:none;
	max-width: 200px;
    display: inline-block;
    /* word-wrap: break-word; */
    white-space: nowrap;
    -o-text-overflow: ellipsis;
    -moz-text-overflow: ellipsis;
    text-overflow: ellipsis;
    overflow: hidden;
}
.div_top1_span:hover{
	text-decoration: underline;
}
.div_top1_span2{
	float: right;
	color:#757b85;
	font:13px/24px 'Microsoft Yahei';
	text-decoration:none;
	margin-left:8px;
}
.div_top1_span2:hover{
	text-decoration: underline;
}
.div_top2{
	margin:0px;
  	padding:0px;
  	width:100%;
  	height:75px;
  	text-align:center;
  	background:#ffffff;
}
.title{
	width: 1140px;
    height: 75px;
    margin: auto;
    background-color: white;
    font-family: "microsoft yahei";
}
.img-ltop{
	float:left; 
	display: block; 
	border: none; 
	display: block; 
	height: 75px;
	margin-left:23px;
	cursor:pointer;
}
.table_headline{
	margin:0px auto;
	height:100%;
	border:0px;
	border-spacing: 0;
	border-collapse: collapse;
	margin-right:14px;
	float:right;
}
.table_headline li{
	
}
.table_headline a{
	color:#000;
	font:18px/25px 'Microsoft yahei';
	text-decoration: none;
}
.table_headline a:link{
	text-decoration: none;
}
.headline_press{
	width:120px;
	text-align:center;
}
.headline_press span{
	display:inline-block;
	margin:0 auto;/**div在td中居中**/
	color:#fff;
	text-align:center;
    background-color: #41C651;
    height: 35px;
    width: 95px;
    border-radius: 5px;
    line-height: 35px;
}
.headline_normal{
	width:120px;
	text-align:center;
}
.headline_normal span{
	display:inline-block;
	margin:0 auto;
	width:92px;
	text-align:center;
}
.headline_normal a:hover{
	color:#73D27C;
	cursor:pointer;
}
/*******查看图片的样式开始*****/
.watchimg_a{
	text-decoration:none;
	outline:none;
}
.watchimg_a img{
	border:0; 
	max-height: 280px;
	max-width:600px;
}
.img_a{
