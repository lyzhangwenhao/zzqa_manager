/**公共表格样式开始**/
.device_tab{
	margin:0px auto;
	table-layout: fixed;
	border-collapse: collapse;
	border-bottom:1px solid #e6e6e6;
	border-left:1px solid #e6e6e6;
	width:95%;
}
.device_tab tr{
	height:40px;
}
.device_tab td{
	text-align:center;
	white-space:nowrap;
	-o-text-overflow:ellipsis;
	-moz-text-overflow: ellipsis;
    text-overflow:ellipsis;
    overflow:hidden;
    border-top:1px solid #e6e6e6;
	border-right:1px solid #fff;
}
.device_tab1{
	margin:0px auto;
	table-layout:fixed;
	border-collapse: collapse;
	width:100%;
}
.device_tab1 tr{
	height:40px;
}
.device_tab1 td{
	text-align:center;
	white-space:nowrap;
	-o-text-overflow:ellipsis;
	-moz-text-overflow: ellipsis;
    text-overflow:ellipsis;
    overflow:hidden;
    border-top:1px solid #e6e6e6;
	border-right:1px solid #fff;
}
.tab_tr1{
	background:#799DD8;
}
.tab_tr1 td{
	color:#fff;
