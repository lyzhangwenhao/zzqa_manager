.shipping_tab{
	margin: 10px auto 0px auto;
    table-layout: fixed;
    border-collapse: collapse;
    width: 100%;
    
}
.shipping_tab tr{
	border-top:1px solid #c0c0c0;
	border-left:1px solid #c0c0c0;
}
.shipping_tab tr:not(:nth-of-type(1)) td{
	white-space: nowrap;
    -o-text-overflow: ellipsis;
    -moz-text-overflow: ellipsis;
    text-overflow: ellipsis;
    overflow: hidden;
}
.shipping_tr_title{
	background:#ADDBE8;
}
.shipping_tr_title td{
	padding:8px 5px;
	color:#000;
	font-weight:bold;
	font:14px/15px 'SimSun';
}
.shipping_tab td{
	border-bottom:1px solid #c0c0c0;
	border-right:1px solid #c0c0c0;
}
.shipping_tr_content{
	cursor:pointer;
}
.shipping_tr_content:nth-child(odd) {
    background: #EAEFF7;
}
.shipping_tr_content:nth-child(even) {
    background: #F5F9FD;
}
.shipping_tr_content:hover{
	background-color:#dfebfe;
}
.shipping_tr_content td{
	padding:10px 5px;
	color:#000;
	font:13px/20px 'SimSun';
}
#shipping_tr_sum{
	border:0;
}
#shipping_tr_sum td{
	border:0;
    padding:10px 0px;
	color:#000;
	font:13px/20px 'SimSun';
}
